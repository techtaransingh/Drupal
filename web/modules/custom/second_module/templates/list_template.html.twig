{{ dump(people) }}

<table>
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">Phone</th>
    </tr>
  </thead>
  <tbody>
   {% for key,value in people %}
   
    <tr>
     
      <td>{{ value.name }}</td>
      <td>{{ value.age }}</td>
      <td>{{ value.phone }}</td>

    </tr>
    
{% endfor %}
    
  </tbody>
</table>

